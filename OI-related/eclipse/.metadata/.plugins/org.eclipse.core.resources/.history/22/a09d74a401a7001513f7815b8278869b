package spoj_wtf_test;
import java.util.*;
import java.io.*;

public class spoj_wtf_test {
	public static void main(String []args) throws Exception{
		String res = "1\n2\n88\n42\n99";
		Scanner cin = new Scanner(new ByteArrayInputStream(res.getBytes()));
		int temp;
		temp = cin.nextInt();
		while (temp != 42) {
			System.out.println(temp);
			temp = cin.nextInt();
		}
		cin.close();
	}
}